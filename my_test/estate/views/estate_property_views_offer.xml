<odoo>
    <record id="estate_property_view_tree_offer" model="ir.ui.view">
        <field name="name">estate.property.tree.offer</field>
        <field name="model">estate.property.offer</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="price"/>
                <field name="partner_id"/>
                <field name="validity"/>
                <field name="date_deadline"/>
                <field name="status"/>
            </list>
        </field>
    </record>



</odoo>
